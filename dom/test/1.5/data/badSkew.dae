  <library_nodes>
    <node id="my-node">
      <!-- Skew is supposed to have 7 values -->
      <skew sid="tooFew">45 1 0 0</skew>
      <!-- Too few values -->
      <skew sid="justRight">45 1 0 0 1 0 0</skew>
      <!-- Correct number of values -->
