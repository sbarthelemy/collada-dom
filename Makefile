all:
	make os=ps3 project=dom -C dom
	make os=ps3 project=fx -C fx
	make os=ps3 project=rt -C rt
	make os=ps3 project=viewer -C viewer
	
clean:
	make os=ps3 project=dom -C dom clean
	make os=ps3 project=fx -C fx clean
	make os=ps3 project=rt -C rt clean
	make os=ps3 project=viewer -C viewer clean